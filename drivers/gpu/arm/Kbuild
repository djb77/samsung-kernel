#obj-$(CONFIG_MALI_T6XX) += t6xx/
#obj-$(CONFIG_MALI_MIDGARD) += midgard/
#obj-$(CONFIG_MALI_MIDGARD_WK04) += midgard_wk04/
#obj-$(CONFIG_MALI400) += mali400/
ifeq ($(CONFIG_MALI_T7XX),y)
	ifeq ($(CONFIG_MALI_R5P0),y)
		obj-y += t7xx/r5p0/
	endif
	ifeq ($(CONFIG_MALI_R7P0),y)
		obj-y += t7xx/r7p0/
	endif
	ifeq ($(CONFIG_MALI_R8P0),y)
		obj-y += t7xx/r8p0/
	endif
endif
ifeq ($(CONFIG_MALI_T72X),y)
	ifeq ($(CONFIG_MALI_R5P0_T72X),y)
		obj-y += t72x/r5p0/
	endif
endif
ifeq ($(CONFIG_MALI_T8XX),y)
	ifeq ($(CONFIG_MALI_R5P0_REL),y)
		obj-y += t8xx/r5p0/
	endif
	ifeq ($(CONFIG_MALI_R7P0),y)
		obj-y += t8xx/r7p0/
	endif
	ifeq ($(CONFIG_MALI_R8P0),y)
		obj-y += t8xx/r8p0/
	endif
	ifeq ($(CONFIG_MALI_R9P0),y)
		obj-y += t8xx/r9p0/
	endif
endif

