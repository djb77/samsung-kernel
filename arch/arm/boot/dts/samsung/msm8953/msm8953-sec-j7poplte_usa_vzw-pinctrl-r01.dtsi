				};
			};
		};

		pmx_mdss: pmx_mdss {
			mdss_dsi_active: mdss_dsi_active {
				mux {
					pins =
						/* LCD */
						"gpio50", "gpio94",
						/* BLIC */
						"gpio41", "gpio22", "gpio23";

					function = "gpio";
				};

				config {
					pins =
						/* LCD */
						"gpio50", "gpio94",
						/* BLIC */
						"gpio41", "gpio22", "gpio23";

					drive-strength = <8>; /* 8 mA */
					bias-pull-down;
				};
			};

			mdss_dsi_suspend: mdss_dsi_suspend {
				mux {
					pins =
						/* LCD */
						"gpio50", "gpio94",
						/* BLIC */
						"gpio41", "gpio22", "gpio23";

					function = "gpio";
				};

				config {
					pins =
						/* LCD */
						"gpio50", "gpio94",
						/* BLIC */
						"gpio41", "gpio22", "gpio23";

					drive-strength = <2>; /* 2 mA */
					bias-pull-down; /* pull down */
				};
			};

			mdss_dsi_esd_active: mdss_dsi_esd_active {
				mux {
					pins =
						/* ESD */
						"gpio13";

					function = "gpio";
				};

				config {
					pins =
						/* ESD */
						"gpio13";

					drive-strength = <8>; /* 8 mA */
					bias-disable;         /* No PULL */
					input-enable;
				};
			};

			mdss_dsi_esd_suspend: mdss_dsi_esd_suspend {
				mux {
