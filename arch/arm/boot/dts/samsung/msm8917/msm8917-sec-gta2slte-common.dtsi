	tsp_io: tsp_io {
		compatible = "regulator-fixed";
		regulator-name = "tsp_io";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		gpio = <&tlmm 91 0>;
		enable-active-high;
	};

	tsp_avdd: tsp_avdd {
		compatible = "regulator-fixed";
		regulator-name = "tsp_avdd";
		regulator-min-microvolt = <3000000>;
		regulator-max-microvolt = <3000000>;
		gpio = <&tlmm 92 0>;
		enable-active-high;
	};

	i2c_3: i2c@78b7000 { /* BLSP1 QUP3 */
		status = "ok";

		touchscreen@49 {
			compatible = "stm,fts_touch";
			reg = <0x49>;
			pinctrl-names = "on_state", "off_state", "default";
			pinctrl-0 = <&tsp_int_active &tsp_vdd_active>;
			pinctrl-1 = <&tsp_int_suspend &tsp_vdd_suspend>;
			pinctrl-2 = <&tsp_id_default &tsp_vdd_default>;
			stm,tsp-id1 = <&tlmm 24 0>;
			stm,tsp-id2 = <&tlmm 43 0>;
			stm,irq_gpio = <&tlmm 65 0>;
			stm,irq_type = <8200>;
			stm,max_coords = <1600 2560>;	/* x y */
			stm,regulator_dvdd = "tsp_io";
			stm,regulator_avdd = "tsp_avdd";
			stm,pat_function = <2>;
			stm,afe_base = <0xD782>;
			stm,project_name = "TabA2S", "T385";
			stm,firmware_name = "tsp_stm/fts8cd56_taba2s.fw";
			stm,num_touchkey = <2>;
		};
