		compatible = "samsung,exynos-iommu-bus";
		#dma-address-cells = <1>;
		#dma-size-cells = <1>;
		/* start address, size */
		dma-window = <0x30000000 0xA0000000>;

		domain-clients = <&vpu>;
	};

