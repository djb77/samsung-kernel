	mipi_phy_csis_m4s4_m1s2s2: dphy_m4s4_m1s2s2_csis@0x12C21050 {
		compatible = "samsung,mipi-phy-csis";
		samsung,pmu-syscon = <&pmu_system_controller>;
		isolation = <0x70C 0x710 0x730 0x730>; /* PMU address offset */
		reg = <0x0 0x12C21050 0x4>; /* SYSREG address for reset */
		reset = <0 1 2 3>; /* reset bit */
		#phy-cells = <1>;
	};

	mipi_phy_csis_m4s4: dphy_m4s4_csis@0x12C21050 {
		compatible = "samsung,mipi-phy-csis";
