	acpm {
		compatible = "samsung,exynos-acpm";
		#address-cells = <2>;
		#size-cells = <1>;
		reg = <0x0 0x16488000 0x1000>;		/* PMU_ALIVE */
		acpm-ipc-channel = <4>;
		dynamic-plugins {
			dvfs-plugin {
				plugin-id = <3>;
				fw-name = "emulator8895_acpm_dvfs.fw";
			};
		};
	};

	acpm_ipc {
		compatible = "samsung,exynos-acpm-ipc";
		#address-cells = <2>;
		#size-cells = <1>;
		interrupts = <0 39 0>;
		reg = <0x0 0x16440000 0x1000>,		/* AP2APM MAILBOX */
			<0x0 0x16500000 0xB400>;	/* APM SRAM */
		initdata-base = <0x2850>;
		num-timestamps = <32>;
		debug-log-level = <0>;
		logging-period = <500>;
		dump-base = <0x16500000>;
		dump-size = <0xB400>;
