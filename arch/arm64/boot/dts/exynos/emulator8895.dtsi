			  "109e0000.spi",		/* [35] spi_0    */
			  "109e0000.spi",		/* [36] spi_1    */
			  "10440000.spi",		/* [37] spi_2    */
			  "10460000.spi",		/* [38] spi_3    */
			  "10480000.spi",		/* [39] spi_4    */
			  "104a0000.spi",		/* [40] spi_5    */
