			  "10940000.spi",		/* [49] spi_14   */
			  "10960000.spi",		/* [50] spi_15   */
			  "11120000.ufs",		/* [51] ufs      */
			  "11500000.dwmmc2",		/* [52] dwmmc2   */
			  "10c00000.usb",		/* [53] usb      */
			  "14040000.mailbox",		/* [54] mailbox  */
			  "11700000.pcie0",		/* [55] pcie0    */
			  "11710000.pcie1",		/* [56] pcie1    */
			  "pd-abox",			/* [57] pd-abox  */
